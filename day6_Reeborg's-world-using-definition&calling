#def digunakan untuk membuat definisi function yang akan digunakan berkali kali serta memudahkan membuat instruksi
def turn_right():
    turn_left()
    turn_left()
    turn_left()
    
def turn_around():
    turn_left()
    turn_left()

def square():
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_right()
    move()
    
square()
